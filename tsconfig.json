{
  "compilerOptions": {
    "lib": ["es2017", "dom"],
    "sourceMap": true,
    "module": "commonjs",
    "target": "es5",
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "typeRoots": ["./node_modules/@types"],
    "declaration": false,
    "resolveJsonModule": true,
    "keyofStringsOnly": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "strict": false,
    "strictNullChecks": false
  }
}
